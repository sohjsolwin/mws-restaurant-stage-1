{
    "parserOptions":{
      "sourceType": "module"  
    },
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "globals":{
        "module":false,
        "L":false
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended"
}